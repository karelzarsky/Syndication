@model TradeTip
<div class="col-lg-1">@Model.ArticleID</div>
<div class="col-lg-1">@Model.Published.ToLocalTime().ToString("dd.MM. HH:mm")</div>
<div class="col-lg-1">@Model.Score</div>
<div class="col-lg-1"><strong>@Model.Ticker</strong></div>
<div class="col-lg-8"><a href="@Model.Link">@Model.Title</a></div>
