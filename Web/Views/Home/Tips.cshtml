@model SyndicationWeb.ViewModels.TipsViewModel
@{
    ViewData["Title"] = "Today Tips";
}
<div class="container-fluid">
    <div class="row">
        <h1 class="text-center">Todays trade tips <span class="showopacity glyphicon glyphicon-thumbs-up" /></h1>
    </div>
    <div class="row bg-primary">
        <div class="col-lg-1 col-ms-6"><strong>ArticleID</strong></div>
        <div class="col-lg-1 col-ms-6"><strong>Score</strong></div>
        <div class="col-lg-1 col-ms-6"><strong>Received</strong></div>
        <div class="col-lg-1 col-ms-6"><strong>Published</strong></div>
        <div class="col-lg-1 col-ms-6"><strong>Signal</strong></div>
        <div class="col-lg-1 col-ms-6"><strong>Ticker</strong></div>
        <div class="col-lg-6 col-ms-12"><strong>Title</strong></div>
    </div>
    @foreach (var tip in Model.Tips)
    {
        @Html.Partial("_Tip", tip)
    }
</div>
